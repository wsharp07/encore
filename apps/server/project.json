{
  "root": "apps/server",
  "sourceRoot": "apps/server/src",
  "projectType": "application",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "python main.py",
        "cwd": "apps/server/src"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pylint src",
        "cwd": "apps/server"
      }
    }
  }
}